<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>UtilisateurServelet</servlet-name>
        <servlet-class>servelet.UtilisateurServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SalleCultureServelet</servlet-name>
        <servlet-class>servelet.SalleCultureServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>preparedSalleCutlureServelet</servlet-name>
        <servlet-class>servelet.preparedSalleCutlureServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PlanteServelet</servlet-name>
        <servlet-class>servelet.PlanteServelet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>preparedPlanteServelet</servlet-name>
        <servlet-class>servelet.preparedPlanteServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UtilisateurServelet</servlet-name>
        <url-pattern>/UtilisateurServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SalleCultureServelet</servlet-name>
        <url-pattern>/SalleCultureServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>preparedSalleCutlureServelet</servlet-name>
        <url-pattern>/preparedSalleCutlureServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PlanteServelet</servlet-name>
        <url-pattern>/PlanteServelet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>preparedPlanteServelet</servlet-name>
        <url-pattern>/preparedPlanteServelet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
